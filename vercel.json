{
  "functions": {
    "api/server.ts": {
      "runtime": "@vercel/node@2.0.0",
      "includeFiles": "public/**/*",
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.ts"
    }
  ]
}
